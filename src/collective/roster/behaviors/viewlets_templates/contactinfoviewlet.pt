<dl class="personContactInfo" i18n:domain="collective.roster">
    <dt i18n:translate="email">email:</dt>
    <dd><a tal:attributes="href string:mailto:${view/w/IContactInfo.email/value}"
      tal:content="structure view/w/IContactInfo.email/render" />
    </dd>

    <dt i18n:translate="phone">Phone:</dt>
    <dd><a tal:attributes="href string:tel:${view/w/IContactInfo.phone_number/value}"
      tal:content="structure view/w/IContactInfo.phone_number/render" />
    </dd>

    <dt i18n:translate="short_number">Short number:</dt>
    <dd><a tal:attributes="href string:tel:${view/w/IContactInfo.short_number/value}"
      tal:content="structure view/w/IContactInfo.short_number/render" />
    </dd>
</dl>
