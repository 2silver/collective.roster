<dl class="personOfficeInfo" i18n:domain="collective.roster">

    <div tal:condition="view/w/IOfficeInfo.short_number/value" >
      <dt i18n:translate="short_number">Short number:</dt>
      <dd>
        <a tal:attributes="href string:tel:${view/w/IOfficeInfo.short_number/value}"
          tal:content="structure view/w/IOfficeInfo.short_number/render" />
      </dd>
    </div>

    <div tal:condition="view/w/IOfficeInfo.room/value" >
      <dt i18n:translate="room">Room:</dt>
      <dd tal:content="structure view/w/IOfficeInfo.room/render" />
    </div>
</dl>
